<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller -->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
			<st id="ST" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>

    <!--New Generalized definitions -->

    <!--Generic Lamp/Bulb -->
     <nodeDef id="STDLAMP" nls="STDL">
        <editors />
        <sts>
            <st id="ST" editor="ONOFF" />
            <st id="OL" editor="OL"/>
        </sts>
        <cmds>
            <sends>
                <cmd id="DON" />
                <cmd id="DOF" />
                <!-- <cmd id="OL" /> -->
            </sends>
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="QUERY" />
                <cmd id="SET_LVL">
                    <p id="" editor="OL"  init="OL"/>
                </cmd>
            </accepts>
        </cmds>
    </nodeDef>
    


</nodeDefs>